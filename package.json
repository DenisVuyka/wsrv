  "devDependencies": {
    "chai": "3.5.0",
    "chai-http": "3.0.0",
    "eslint": "3.16.1",
    "eslint-config-standard": "6.2.1",
    "eslint-plugin-promise": "3.4.2",
    "eslint-plugin-standard": "2.0.1",
    "install": "0.8.7",
    "istanbul": "0.4.5",
    "mocha": "3.2.0",
    "sinon": "1.17.4",
    "sinon-chai": "2.8.0"
  }
