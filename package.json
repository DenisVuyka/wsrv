    "eslint": "eslint ./bin/wsrv && eslint ./lib/*.js && eslint ./index.js",
    "coverage": "node ./bin/wsrv ./coverage/lcov-report -o"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DenisVuyka/wsrv.git"
  },
  "keywords": [
    "development",
    "server",
    "spa",
    "static",
    "web",
    "http"
  ],
  "author": "Denys Vuika <denys.vuika@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/DenisVuyka/wsrv/issues"
  },
  "homepage": "https://github.com/DenisVuyka/wsrv#readme",
  "bin": {
    "wsrv": "./bin/wsrv"
  },
  "dependencies": {
    "chokidar": "1.6.1",
    "good": "7.1.0",
    "good-console": "6.4.0",
    "good-squeeze": "5.0.1",
    "h2o2": "6.0.0",
    "hapi": "16.1.0",
    "inert": "4.1.0",
    "livereload-js": "2.2.2",
